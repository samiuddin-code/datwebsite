        "@/banners/*": ["components/banners/*"],
        "@/blogs/*": ["components/blogs/*"],
        "@/footer/*": ["components/footer/*"],
        "@/cookies/*": ["components/cookies/*"],
        "@/header/*": ["components/header/*"],
        "@/navigation/*": ["components/header/navigation/*"],
        "@/helpers/*": ["components/helpers/*"],
        "@/herosection/*": ["components/herosection/*"],
        "@/icons/*": ["components/icons/*"],
        "@/service/*": ["components/service/*"],
        "@/swiper/*": ["components/swiper/*"],
        "@/ui/*": ["components/ui/*"],
        "@/forms/*": ["components/ui/forms/*"],
        "@/modals/*": ["components/ui/modals/*"],
